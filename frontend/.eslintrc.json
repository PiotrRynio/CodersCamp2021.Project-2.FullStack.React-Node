{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": 0,
    "no-restricted-syntax": "off",
    "no-param-reassign": [2, { "props": false }],
    "prettier/prettier": "warn"
  }
}